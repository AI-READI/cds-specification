{
  "name": "fairdataihub-docs",
  "version": "2.6.0",
  "description": "Technical documentation for FAIR Data Innovations Hub",
  "main": "index.js",
  "author": "Sanjay Soundarajan <sanjay.16@live.com>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vitepress dev docs --port 3000",
    "docs:dev": "vitepress dev docs --port 3000",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "format": "prettier --ignore-path .gitignore \"./**/*.+(ts|js|jsx|tsx|md)\" --write",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "./**/*.{ts,js,jsx,tsx,md}": [
      "yarn format"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@fairdataihub/config": "1.2.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "devmoji": "2.3.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "vitepress": "1.0.0-alpha.10",
    "vue": "3.2.37"
  },
  "dependencies": {
    "mermaid": "^9.1.3",
    "vitepress-plugin-mermaid": "^1.0.1"
  }
}
